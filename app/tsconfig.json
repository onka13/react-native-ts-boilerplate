{
	"compilerOptions": {
		//"target": "es2015",
		//"module": "es2015",
		"noEmit": true,
		"sourceMap": true,
		"experimentalDecorators": true,
		"jsx": "react-native",
		"lib": ["es2015", "esnext"],
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"noEmitHelpers": true,
		"importHelpers": true,
		"strict": true,
		//"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"resolveJsonModule": true,		
		"baseUrl": ".",
		"paths": {
			"infra/*": ["../infra/*"],
			"*": ["*"]
		}
	},
	//"include": ["./src/**/*"]
}
